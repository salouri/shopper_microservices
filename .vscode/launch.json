  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Registry",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/registry-service/bin/start.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Shopper",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/shopper-service/bin/start.js"
